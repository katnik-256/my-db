    var y = document.forms['Login']['.password'].value;

}

//objects

var  person={
    Name : 'Kato',
    Age: 90,
    job:'software developer',
    Residence:'Kampala'
}
var x =person;
x.Residence = 'Nansana';
document.getElementById('person').innerHTML=
person.Name +" " +" "+ "lives in"+" "+person.Residence;

function myFunction(a,b,c){
    return arguments.length;
}
document.getElementById('demo').innerHTML=myFunction(3,4,5);

function adMe(a,b,c){
    return (a+b+c);
}
window.adMe(2,5,6);
document.getElementById('add').innerHTML=adMe(2,5,6);

var people={
    fullName: function(){
        return this.firstName + " "+ this.lastName;
    }

}

var person1={
    firstName:'kato',
    lastName:'nicholas'
}
var person2={
    firstName :'elvis',
    lastName:'baguma'
}
x = people.fullName.call(person2);
document.getElementById('g').innerHTML = x;


